<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/bluegray_901"/>
            <corners android:radius="@dimen/_20pxh"/>
            <stroke android:color="@color/teal_A400" android:width="@dimen/_1pxh"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/teal_A400"/>
            <corners android:radius="@dimen/_20pxh"/>
        </shape>
    </item>
</selector>
