                    android:id="@+id/viewRectangle56"
                    android:layout_height="@dimen/_52pxh"
                    android:layout_marginBottom="@dimen/_658pxv" android:layout_width="@dimen/_6pxh"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearGroup1075"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_weight="0.98"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearButtons"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_5pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNext7Days"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{dailyWeatherNextVM.dailyWeatherNextModel.txtNext7Days}"
                            style="@style/txtOpenSansromanbold20_1" tools:text="@string/lbl_next_7_days"/>
                        <FrameLayout android:id="@+id/frameGroup1080"
                            android:layout_height="@dimen/_24pxv"
                            android:layout_marginStart="@dimen/_66pxh" android:layout_width="@dimen/_140pxh">
