                                android:id="@+id/frameGroup1082"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_24pxv" android:layout_width="@dimen/_140pxh">
                                <ImageView android:id="@+id/imageSignin"
                                    android:layout_gravity="end"
                                    android:layout_height="@dimen/_24pxh"
                                    android:layout_marginStart="@dimen/_10pxh"
                                    android:layout_width="@dimen/_91pxh"
                                    android:scaleType="fitXY" android:src="@drawable/img_signin_6"/>
