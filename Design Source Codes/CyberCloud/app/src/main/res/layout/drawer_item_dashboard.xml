<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="drawerItemDashboardModel" type="com.application.app.modules.itemdashboard.data.model.DrawerItemDashboardModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearDashboard"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylebluegray_903">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup1158"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylebluegray_903">
            <FrameLayout android:id="@+id/frameGroup1160"
                android:layout_gravity="start"
                android:layout_height="@dimen/_209pxv" android:layout_width="@dimen/_324pxh">
                <ImageView android:id="@+id/imageBG"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_209pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_bg_1}" tools:src="@drawable/img_bg_1"/>
                <LinearLayout android:id="@+id/linearGroup1154"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_65pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_65pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_user}" tools:src="@drawable/img_user"/>
                    <TextView android:id="@+id/txtPatrickBacalso"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:text="@{drawerItemDashboardModel.txtPatrickBacalso}"
                        style="@style/txtOpenSansromanbold20_1" tools:text="@string/msg_patrick_bacalso"/>
                </LinearLayout>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearGroup1152"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:id="@+id/txtWeather"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_width="wrap_content"
                    android:text="@{drawerItemDashboardModel.txtWeather}"
                    style="@style/txtOpenSansromanregular20_2" tools:text="@string/lbl_weather"/>
                <FrameLayout android:id="@+id/frameGroup1162"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_63pxv"
                    android:layout_marginEnd="@dimen/_73pxh"
                    android:layout_marginStart="@dimen/_73pxh"
                    android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_152pxh">
                    <ImageView android:id="@+id/imageRectangle12"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_63pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_78pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_rectangle12"/>
                    <TextView android:id="@+id/txtCF"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:text="@{drawerItemDashboardModel.txtCF}"
                        style="@style/txtOpenSansromanbold25" tools:text="@string/lbl_c_f"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAddtohomescre"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCloudy1"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_32pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_cloudy1"/>
                <TextView android:id="@+id/txtAddToHomeScr"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp"
                    android:text="@{drawerItemDashboardModel.txtAddToHomeScr}"
                    style="@style/txtOpenSansromanbold20_1" tools:text="@string/msg_add_to_home_scr"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameGroup1164"
                android:layout_gravity="start"
                android:layout_height="@dimen/_75pxv"
                android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_324pxh">
                <ImageView android:id="@+id/imageRectangle11"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_75pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY" android:src="@drawable/img_rectangle11"/>
                <LinearLayout android:id="@+id/linearGroup1153"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageEditing1"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_editing1"/>
                    <TextView android:id="@+id/txtEditProfile"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_weight="0.87"
                        android:layout_width="0dp"
                        android:text="@{drawerItemDashboardModel.txtEditProfile}"
                        style="@style/txtOpenSansromanbold20_1" tools:text="@string/lbl_edit_profile"/>
                </LinearLayout>
            </FrameLayout>
            <View android:background="@color/cyan_50"
                android:id="@+id/lineSeparator"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_270pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSettings"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSetting11"
                    android:layout_height="@dimen/_31pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_setting11"/>
                <TextView android:id="@+id/txtSettings"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp"
                    android:text="@{drawerItemDashboardModel.txtSettings}"
                    style="@style/txtOpenSansromanbold20_1" tools:text="@string/lbl_settings"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearFeedback"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFeedback"
                    android:layout_height="@dimen/_31pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_feedback"/>
                <TextView android:id="@+id/txtSendFeedback"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp"
                    android:text="@{drawerItemDashboardModel.txtSendFeedback}"
                    style="@style/txtOpenSansromanbold20_1" tools:text="@string/lbl_send_feedback"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearHelp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_108pxv"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageQuestion"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_29pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_question"/>
                <TextView android:id="@+id/txtHelp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_weight="0.89"
                    android:layout_width="0dp"
                    android:text="@{drawerItemDashboardModel.txtHelp}"
                    style="@style/txtOpenSansromanbold20_1" tools:text="@string/lbl_help"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
