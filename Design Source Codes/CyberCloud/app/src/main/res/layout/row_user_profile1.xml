<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="userProfile1RowModel" type="com.application.app.modules.userprofile.data.model.UserProfile1RowModel"/>
    </data>
    <LinearLayout android:clickable="true"
        android:id="@+id/linearAboutUs"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearGroup209"
            android:layout_height="wrap_content"
            android:layout_weight="0.95" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageInfo"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY" android:src="@drawable/img_info"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAboutUs"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{userProfile1RowModel.txtAboutUs}"
                style="@style/txtOpenSansromanregular20_2" tools:text="@string/lbl_about_us"/>
        </LinearLayout>
        <ImageView android:clickable="true"
            android:id="@+id/imageRightarrow3"
            android:layout_height="@dimen/_15pxh"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginStart="@dimen/_59pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="@dimen/_15pxh"
            android:scaleType="fitXY" android:src="@drawable/img_rightarrow3"/>
    </LinearLayout>
</layout>
