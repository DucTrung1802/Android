<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="emergencyContacts2VM" type="com.application.app.modules.emergencycontacts2.data.viewmodel.EmergencyContacts2VM"/>
    </data>
    <LinearLayout android:id="@+id/linearGroup1369"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <FrameLayout android:id="@+id/frameGroup328"
                android:layout_height="@dimen/_100pxv" android:layout_width="match_parent">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmergencyConta"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_27pxv"
                    android:layout_marginEnd="@dimen/_67pxh"
                    android:layout_marginStart="@dimen/_67pxh"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{emergencyContacts2VM.emergencyContacts2Model.txtEmergencyConta}"
                    style="@style/txtOpenSansromanbold23_1" tools:text="@string/msg_emergency_conta"/>
                <FrameLayout android:id="@+id/frameGroup330"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_100pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageRectangle4"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_100pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY" android:src="@drawable/img_rectangle4_1"/>
                    <LinearLayout android:id="@+id/linearGroup315"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_36pxv"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_marginTop="@dimen/_33pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_31pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_menu_1"/>
                        <ImageView android:id="@+id/imageSearch"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginStart="@dimen/_278pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_search"/>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearGroup326"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <com.google.android.material.tabs.TabLayout
                    android:gravity="end"
                    android:id="@+id/tabLayoutGroup27"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_width="match_parent"
                    app:tabBackground="@drawable/selector_bg_bluegray_901_border_teal_a400_radius_20"
                    app:tabIndicatorColor="@android:color/transparent"
                    app:tabIndicatorHeight="0dp" app:tabMode="fixed"
                    app:tabSelectedTextColor="@color/cyan_50" app:tabTextColor="@color/bluegray_901"/>
                <androidx.viewpager2.widget.ViewPager2
                    android:gravity="start"
                    android:id="@+id/viewPagerGroup334"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_weight="1" android:layout_width="match_parent"/>
            </LinearLayout>
        </ScrollView>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearRectangle3"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylebluegray_904">
            <LinearLayout android:id="@+id/linearGroup331"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_52pxv"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHut1"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_hut1"/>
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_47pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_calendar"/>
                <ImageView android:id="@+id/imageFloods21"
                    android:layout_height="@dimen/_45pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_47pxh"
                    android:layout_width="@dimen/_45pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_floods21"/>
                <ImageView android:id="@+id/imageRadar"
                    android:layout_height="@dimen/_39pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_39pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_radar"/>
                <ImageView android:id="@+id/imageEmergencycall"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_emergencycall"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
